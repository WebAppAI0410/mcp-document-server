{
  "name": "mcp-document-server",
  "version": "1.0.0",
  "description": "MCP Document Server for reducing app development errors by providing version-specific documentation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "add-source": "tsx src/cli/add-source.ts"
  },
  "keywords": [
    "mcp",
    "documentation",
    "llm",
    "claude",
    "vector-search"
  ],
  "author": "WebAppAI0410",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.25.2",
    "html-to-text": "^9.0.5",
    "marked": "^12.0.0",
    "openai": "^4.46.0",
    "p-queue": "^8.0.1",
    "pg": "^8.11.5",
    "pgvector": "^0.1.8",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "sqlite3": "^5.1.7",
    "turndown": "^7.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.5",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}